package steps

import (
	"fmt"
	"checkup/errors"
)

func init() {
	StepList["waitForTextPresent"] = waitForTextPresent
}

func waitForTextPresent(_url interface{}) {

	url := _url.(string)

	fmt.Print("[waitForTextPresent]: " + url + " ")
	err := WD.Get(url)
	errors.Fatal(err)
	fmt.Print("SUCCESS")
	fmt.Println("")

}
