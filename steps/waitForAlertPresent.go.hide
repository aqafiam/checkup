package steps

import (
	"fmt"
	"checkup/errors"
)

func init() {
	StepList["waitForAlertPresent"] = waitForAlertPresent
}

func waitForAlertPresent(_url interface{}) {

	url := _url.(string)

	fmt.Print("[waitForAlertPresent]: " + url + " ")
	err := WD.Get(url)
	errors.Fatal(err)
	fmt.Print("SUCCESS")
	fmt.Println("")

}
