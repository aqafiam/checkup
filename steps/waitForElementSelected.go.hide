package steps

import (
	"fmt"
	"checkup/errors"
)

func init() {
	StepList["waitForElementSelected"] = waitForElementSelected
}

func waitForElementSelected(_url interface{}) {

	url := _url.(string)

	fmt.Print("[waitForElementSelected]: " + url + " ")
	err := WD.Get(url)
	errors.Fatal(err)
	fmt.Print("SUCCESS")
	fmt.Println("")

}
